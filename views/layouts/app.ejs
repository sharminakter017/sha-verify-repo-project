<body>



    <%- body %> 
    


   <script src="https://code.jquery.com/jquery-3.6.1.min.js"></script>
   <script src="//cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8" crossorigin="anonymous"></script>

    <script>
        $('.soro-table').DataTable();

        const student_photo = document.querySelector('#upload_photo');
        const preload_photo = document.querySelector('.preloader');

        if (student_photo) {

            student_photo.onchange = (e) => {
            const imageUrl = URL.createObjectURL(e.target.files[0]);
            preload_photo.setAttribute('src' , imageUrl)

        }
            
        }

        const delete_btn = document.querySelectorAll('.delete-btn');
        console.log(delete_btn);

        delete_btn.forEach(item => {
            item.onclick = () => {
                const conf = confirm('Are you sure?')

                if(conf){
                    return true;
                }else{
                    return false;
                }
            }
        })



    </script>
</body>
</html>